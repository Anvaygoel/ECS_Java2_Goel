;
	 

	 	  
	

	
		public static  void randchar (String[] args)
		{	
		// no perameters.
		 
		 {
		 
		 //chose a random charcter in the range "A" tp "Z"

		  
		  int numval;
		  char chatval;
		  
		  System.out.println("pick  a random number from 0 to 25");
		   numval = (int)(Math.random()*26);
		   System.out.println("now add that offset to the value of the letter 'A'");
		     chatval = (char) ('A' + numval);
		 }
		
		
		   return ;
		}
		   



		   
		

		
		 
		 
		 	public static void counter(String[] args)
		 	{
		 		
		 	// parameters are:
		 	 int start = 0;
		 	  int stop = 0;
		 	  
		 	  {
		 	  
		 	  	// counts from start to stop by ones
		 	  	  int ctr;
				
				ctr = start;
				while( ctr <= stop)
				{
					System.out.println(ctr + " ");
					ctr = ctr + 1;
					
				
				
				}
		 	  }
		 	
		 	
		 	 return;
		 	 
		 	 }
		 	 
		 

		
		 		public static void abso(String[] args)
		 		{
		 		//parameters are:
		 		  int value = 0;
		 		  {
		 		  
		 		  //find the absolute value of the parameter
		 		   int absval;
		 		   
		 		   if( value < 0)
		 		   	absval = -value;
		 		   else 
		 		   		absval = value;
		 		  }
		 		   		
		 		   		return;
		 		}
}
		 		   		
		 		   		
		 		

		 		   
		 		  
		 		 
			 
		 	 
		 
		 
	 
	 	 
		
	


